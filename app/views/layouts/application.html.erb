<!DOCTYPE html>
<html>
  <head>
    <title>RaceRecords</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <h1><%= self.params['controller'] if Rails.env == 'development' %> <%= self.params['action'] if Rails.env == 'development' %></h1>
    <%= yield %>
  </body>
</html>
